public class Room {
    private String id;
    private String type;
    private double price;
    private double pax;
    private String status;
    private String desc;

    public Room (String id, String type, double price, double pax, String status, String desc){
        this.id = id;
        this.type = type;
        this.price = price;
        this.pax = pax;
        this.status = status;
        this.desc = desc;
    }

    public String getId(){
        return id;
    }

    public void setId(String id){
        this.id = id;
    }

    public String getType(){
        return type;
    }
    
    public void setType(String type){
        this.type = type;
    }
    
    public double getPrice(){
        return price;
    }

    public void setPrice(double price){
        this.price = price;
    }

    public double getPax(){
        return pax;
    }

    public void setPax(double pax){
        this.pax = pax;
    }

    public String getStatus(){
        return status;
    }

    public void setStatus(String status){
        this.status = status;
    }

    public String getDesc(){
        return desc;
    }

    public void setDesc(String desc){
        this.desc = desc;
    }
}
